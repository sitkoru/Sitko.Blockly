@typeparam TEntity
@inherits IframeBlockComponent<TEntity, AntDesignBlocklyListOptions>
<!--suppress HtmlDeprecatedAttribute -->
<div class="iframe-container-wrapper">
    <div class="iframe-container">
        @* ReSharper disable once Html.Obsolete *@
        <iframe src="@Block.Src"
                width="@(Block.Width > 0 ? Block.Width : null)" height="@(Block.Height > 0 ? Block.Height : null)"
                frameborder="0" allowfullscreen class="iframe-content">
        </iframe>
    </div>
</div>
