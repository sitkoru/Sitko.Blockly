@using FormItem = AntDesign.FormItem
@typeparam TForm
@inherits YoutubeBlockForm<TForm>
<FormItem Label="Ссылка">
    <Input @bind-Value="@Block.Url"/>
</FormItem>
@if (!string.IsNullOrEmpty(Block.YoutubeId))
{
    <div style="text-align: center">
        <iframe frameborder="0"
                allowfullscreen=""
                width="560" height="315"
                src="@Block.Url">
        </iframe>
    </div>
}
